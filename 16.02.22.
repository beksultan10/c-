char val{ 0 };
	cin >> val;
	for (int i = 1; i < 4; i++) {
		if ('\'' < val && val < 'x') {
			val = val + 1;
			cout << val;
		}
		else {
			if ('z' == val) {
				val = val - 26;
			}
			val = val + 1;
			cout << val;
		}


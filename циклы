#include <iostream>
#include <iostream>
int main() {
	for (int i = 1; i < 11; i++) {
		std::cout << "beksultan" << std::endl;
	}


	for (int i = 1; i < 10; i++) {
		std::cout << i << " " << i * i << std::endl;
	}
	std::cout << " " << std::endl;

	int summ, month, procent, evryMonth;
	std::cin >> summ;
	std::cin >> month;
	std::cin >> procent;
    float plate= summ / month;
	for (int i = 1; i <= month; i++) {
		evryMonth = summ / month;
		std::cout << evryMonth << std::endl;
		evryMonth = (evryMonth * 0.14) + plate;
		std::cout << i << ' ' << summ << ' ' << evryMonth << std:: endl;
		summ -= plate;
	}
}
